{
	"compilerOptions": {
		"sourceMap": true,
		"outDir": "build/SODS",
		"declaration": true,
		"rootDir": "src",
		"removeComments": true,
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true
	},
	"include": [
		"src/**/*"
	]
}
